	author = {Rosinger, Elemer Elad},
	eprint = {0710.0226},
	file = {:home/dharma/work/articles/2014/Сверхсветовое движение/cite/authors/Rosinger/Rosinger - 2007 - How Far Should the Principle of Relativity Go.pdf:pdf},
	language = {english},
	month = {oct},
	number = {1},
	pages = {1--55},
	title = {How Far Should the Principle of Relativity Go?},
	url = {http://arxiv.org/abs/0710.0226},
	year = {2007}
}

