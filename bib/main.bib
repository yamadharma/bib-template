@InProceedings{kulyabov_2018_sfm_riemannian-not-sufficient_en,
	eid = {1071713},
	annotation = {Progress in Biomedical Optics and Imaging - Proceedings of SPIE CiteScore 2017 0.48 SJR 2017 0.243 SNIP 2017 0.289},
	location = {Saratov},
	author = {Kulyabov, Dmitry Sergeevich and Korolkova, Anna Vladislavovna and Velieva, Tatyana Refatovna},
	title = {The Riemannian Geometry is not Sufficient for the Geometrization of the Maxwell's Equations},
	booktitle = {Saratov Fall Meeting 2017: Laser Physics and Photonics XVIII; and Computational Biophysics and Analysis of Biomedical Data IV},
	year = {2018},
	editor = {Derbov, Vladimir L. and Postnov, Dmitry E.},
	language = {english},
