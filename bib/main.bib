	issn = {0004-6361},
	journal = {Astronomy and Astrophysics},
	language = {english},
	month = {dec},
	pages = {A92},
	title = {A unifying theory of dark energy and dark matter: Negative masses and matter creation within a modified $\Lambda$CDM framework},
	type = {Article},
