@Article{imputato_2020_revisiting-queue-disciplines-pie_en,
	language = {english},
	eid = {107136},
	abstract = {Bloated buffers in the Internet add signiﬁcant queuing delays and have a direct impact on the user perceived latency. There has been an active interest in addressing the problem of rising queue delays by designing easy-to-deploy and eﬃcient Active Queue Management (AQM) algorithms for bottleneck devices. The real deployment of AQM algorithms is a complex task because the eﬃciency of every algorithm depends on appropriate setting of its parameters. Hence, the design of AQM algorithms is usually entrusted on simulation environments where it is relatively straightforward to evaluate the algorithms with different parameter conﬁgurations. Unfortunately, several factors that affect the eﬃciency of AQM algorithms in real deployment do not manifest during simulations, and therefore, lead to ineﬃcient design of the AQM algorithm. In this paper, we revisit the design considerations of Proportional Integral controller Enhanced (PIE), an algorithm widely considered for network deployment, and extensively evaluate its performance using a Linux based testbed. Our experimental study reveals some performance anomalies in certain circumstances and we prove that they can be attributed to a speciﬁc design choice of PIE, namely the use of the estimated departure rate to compute the expected queuing delay. Therefore, we designed an alternative approach based on packet timestamps, implemented it in the Linux kernel and proved its effectiveness through an experimental campaign.},
	keywords = {category:aqm},
	journaltitle = {Computer Networks},
	timestamp = {2022-10-10 15:35:52 (MSK)},
	author = {Imputato, Pasquale and Avallone, Stefano and Tahiliani, Mohit P. and Ramakrishnan, Gautam},
	title = {Revisiting design choices in queue disciplines: The PIE case},
	journal = {Computer Networks},
	year = {2020},
	volume = {171},
	month = {Apr},
	pages = {107136.1-10},
	issn = {1389-1286},
	doi = {10.1016/j.comnet.2020.107136},
	publisher = {Elsevier BV}
}

