@InProceedings{kulyabov_2018_ceur-ws_2177_eikonal_en,
	location = {Moscow},
	author = {Kulyabov, Dmitry Sergeevich and Gevorkyan, Migran Nelsonovich and Korolkova, Anna Vladislavovna},
	title = {Software Implementation of the Eikonal Equation},
	booktitle = {Proceedings of the Selected Papers of the 8th International Conference "Information and Telecommunication Technologies and Mathematical Modeling of High-Tech Systems" (ITTMM-2018), Moscow, Russia, April 16, 2018},
	year = {2018},
