@InProceedings{kulyabov_2017_ceur-ws_1995_julia,
	annotation = {in Russian},
	location = {Moscow},
	author = {Kulyabov, Dmitry Sergeevich and Gevorkyan, Migran Nelsonovich and Korolkova, Anna Vladislavovna and Sevastianov, Leonid Antonovich},
	title = {Support for Parallel Computing in Julia Language},
	booktitle = {Proceedings of the Selected Papers of the 7th International Conference "Information and Telecommunication Technologies and Mathematical Modeling of High-Tech Systems" (ITTMM-2017), Moscow, Russia, April 24, 2017},
	year = {2017},
	editor = {Kulyabov, Dmitry S. and Samouilov, Konstantin E. and Sevastianov, Leonid A.},
	language = {english},
	volume = {1995},
	series = {CEUR Workshop Proceedings},
	month = {apr},
	pages = {93--99},
	url = {http://ceur-ws.org/Vol-1995/paper-13-979.pdf},
	abstract = {The purpose of this paper is a brief review of tools for parallel computing implemented in the current version of the Julia Language. Julia is a young promising language designed for scientific programming. Before describing directly the parallel programming capabilities of Julia we give a small overview of the main features of the language. We describe the main goals pursued by the authors of the language when it was created, and the ideology that they espoused. Separately discussed the scientific focus of Julia, features that make the language convenient for the needs of mathematical modelling, handling big data and intensive numerical computing. Separately discussed such a feature as Julia's multiple dispatch. This mechanism of language paying a lot of attention. Most built-in functions and operators using multiple dispatch. In the second part of the paper we turn to the description of parallel programming. Julia is under active development, so support for parallel computing will be expanded, and the existing mechanisms may change. However, now Julia provides enough capabilities for writing quite complex programs using parallel computing. The basis for concurrency are processes (parallelism based on threads is not yet available). We describe the basic functions and macros that allow you to parallelize the execution of the functions, cycles and separate blocks of code. As the basis for presentation used the official guide and also the experience obtained by the authors in the process of using language.},
	address = {Moscow},
	annote = {in Russian},
	file = {work/bib/pdf/self/2017/ceur-ws-vol1995-paper-13-979.pdf},
	issn = {1613-0073},
	keywords = {self, project:julia, grant:rfbr:15-07-08795, grant:rfbr:16-07-00556, index:rinc, grant:rudn:5-100, index:scopus},
	mendeley-tags = {Scopus,РИНЦ},
	timestamp = {2020-03-30},
	type = {Conference Paper}
}

@InProceedings{kulyabov_2017_ceur-ws_1995_linearization_aqm,
	location = {Moscow},
	author = {Velieva, Tatyana Refatovna and Korolkova, Anna Vladislavovna and Kulyabov, Dmitry Sergeevich},
	title = {About Linearization of Active Traffic Management Module Model},
	booktitle = {Proceedings of the Selected Papers of the 7th International Conference "Information and Telecommunication Technologies and Mathematical Modeling of High-Tech Systems" (ITTMM-2017), Moscow, Russia, April 24, 2017},
	year = {2017},
	editor = {Kulyabov, Dmitry S. and Samouilov, Konstantin E. and Sevastianov, Leonid A.},
	language = {english},
	volume = {1995},
	series = {CEUR Workshop Proceedings},
	month = {apr},
	pages = {114--124},
	url = {http://ceur-ws.org/Vol-1995/paper-16-980.pdf},
	abstract = {The emergence of self-oscillating modes in data-transmission networks negatively affects characteristics of these networks. As a result the task of identifying zones of self-oscillations' origin and studying the self-oscillation parameters becomes relevant. The study of the self- oscillating modes is complicated by the significant nonlinearity of the original system. The study of self-oscillating modes could simplify the transition to the linearized model; however, the self-oscillating mode disappears during the linearization. As an alternative, it is proposed to use an harmonic linearization approach which takes into consideration both the linearized part of the equations and the nonlinearity that influences them. This paper describes the preparation for the application of the harmonic linearization method, namely the linearization of the original nonlinear model.},
	address = {Moscow},
	file = {work/bib/pdf/self/2017/ceur-ws-vol1995-paper-16-980.pdf},
	keywords = {project:aqm, grant:rfbr:15-07-08795, grant:rfbr:16-07-00556, index:rinc, grant:rudn:5-100, index:scopus, self},
	mendeley-tags = {Scopus,РИНЦ},
	timestamp = {2020-03-30},
	type = {Conference Paper}
}

@InProceedings{kulyabov_2017_ceur-ws_1995_wind,
	location = {Moscow},
	author = {Gevorkyan, Migran Nelsonovich and Demidova, Anastasiya Vyacheslavovna and Korolkova, Anna Vladislavovna and Kulyabov, Dmitry Sergeevich},
	title = {Stochastic Modeling of Wind Turbines},
	booktitle = {Proceedings of the Selected Papers of the 7th International Conference "Information and Telecommunication Technologies and Mathematical Modeling of High-Tech Systems" (ITTMM-2017), Moscow, Russia, April 24, 2017},
	year = {2017},
	editor = {Kulyabov, Dmitry S. and Samouilov, Konstantin E. and Sevastianov, Leonid A.},
	language = {english},
	volume = {1995},
	series = {CEUR Workshop Proceedings},
	month = {apr},
	pages = {100--106},
	url = {http://ceur-ws.org/Vol-1995/paper-14-973.pdf},
	abstract = {In this paper we study statistical data collected from wind turbines located on the territory of the Republic of Poland. Sensors, mounted at three points of the wind power plant, took the readings of speed and wind direction. The readings were taken for 9 months with an interval of 10 minutes. The general direction of research aimed at the construction of the stochastic model that predicts the change in wind speed depending on time. The aim of this work is to find the optimal distribution for the approximation of available statistical data on wind speed. We examine four distributions with heavy tails, namely, the log-normal, gamma,Weibull and beta distribution. Each distribution is parameterized by three parameters (the beta distribution has four). For data processing, we used Python with NumPy, SciPy and Matplotlib. From the SciPy library we used statistics module that contains the function to search the parameters of the distribution provided by method of maximum likelihood. After finding the parameters of the distributions were drawn the graphs of the density distributions, which were verified with the histogram of the frequency distribution. The obtained results allow to assert that all distributions with good accuracy can be used for the purposes of approximation. However, the study quantile-quantile graphs revealed that the Weibull distribution better approximate extreme values. The obtained results are consistent with the data presented in the literature, whereWeibull distribution is often used to approximate the distribution of the wind speed. Our future work will focus on the problem of constructing a stochastic differential equation.},
	address = {Moscow},
	annote = {in Russian},
	file = {work/bib/pdf/self/2017/ceur-ws-vol1995-paper-14-973.pdf},
	keywords = {self, grant:rfbr:15-07-08795, grant:rfbr:16-07-00556, index:rinc, grant:rudn:5-100, index:scopus, index:rinc},
	mendeley-tags = {Scopus,РИНЦ},
	timestamp = {2020-03-30},
	type = {Conference Paper}
}

@InProceedings{kulyabov_2017_ceur_vol2064_aqm,
	location = {Moscow},
	author = {Velieva, Tatyana Refatovna and Zaryadov, Ivan Sergeevich and Korolkova, Anna Vladislavovna and Kulyabov, Dmitry Sergeevich},
	title = {The Criteria for the Study of Occurrence of Self-Oscillation Mode of Active Traffic Management Module},
	booktitle = {2nd International Scientific Conference "Convergent Cognitive Information Technologies", Convergent 2017},
	year = {2017},
	editor = {Shneps-Shneppe, M. and Sukhomlin, V. and Zubareva, E.},
	language = {english},
	volume = {2064},
	series = {CEUR Workshop Proceedings},
	publisher = {CEUR-WS},
	month = {nov},
	pages = {70--78},
	abstract = {In data networks, a negative phenomenon such as global synchronization can occur. It manifests itself in the form of auto-oscillations, which may capture the whole segment of the network. As a result, the network parameters such as the bandwidth, latency, etc. are degraded. The reason for this phenomenon is the interaction of control elements in the network (for example, active traffic management modules) and protocols such as TCP. To prevent this phenomenon, it is proposed to control the parameters of the network control elements (since the prospects of a radical modification of the TCP protocol seem somewhat illus ory to us). Specifically, we are studying the active traffic control modules of the RED type. To isolate the main parameters on which the occurrence of self-oscillations may depend, we assumed to linearize the m odel with its subsequent investigation. However, the standard linearization leads to the disappearance of the self-oscillatory regime. We suggest to use the method of harmonic linearization, which preserves the self-oscillatory regime. So the method of harmonic linearization is briefly considered in our paper. We also highlight the main criteria that can be used to determine the presence of self-oscillations and to det ermine their parameters. The application of the harmonic linearization method is demonstrated with the help of the RED active traffic control module.},
	address = {Moscow},
	annote = {in Russian CEUR Workshop Proceedings CiteScore 2016 0.23 Q4 SNIP 2016 0.246},
	file = {work/bib/pdf/self/2017/ceur-ws-vol2064-paper08.pdf},
	issn = {16130073},
	keywords = {self, grant:rfbr:15-07-08795, grant:rfbr:16-07-00556, index:rinc, grant:rudn:5-100, index:scopus, index:rinc},
	mendeley-tags = {Scopus,РИНЦ},
	timestamp = {2020-03-30},
	type = {Conference Paper}
}

@InProceedings{kulyabov_2017_ceur_vol2064_runge-kutta,
	location = {Moscow},
	author = {Kulyabov, Dmitry Sergeevich and Gevorkyan, Migran Nelsonovich and Demidova, Anastasiya Vyacheslavovna and Korolkova, Anna Vladislavovna and Sevastianov, Leonid Antonovich and Kotukov, M. M.},
	title = {Implementation Difficulties Analysis of Stochastic Numerical Runge-Kutta Methods},
	booktitle = {2nd International Scientific Conference "Convergent Cognitive Information Technologies", Convergent 2017},
	year = {2017},
	editor = {Shneps-Shneppe, M. and Sukhomlin, V. and Zubareva, E.},
	language = {english},
	volume = {2064},
	series = {CEUR Workshop Proceedings},
	publisher = {CEUR-WS},
	month = {nov},
	pages = {28--40},
	abstract = {This article discusses stochastic numerical methods of Runge-Kutta methods with weak and strong convergences for systems of stochastic differential equations in the Ito form. At first section we give a brief review of main works on the topic. In next section we make short introduction into theory of stochastic numerical methods and facts from the theory of stochastic differential equations. Due to the fact th at the analytical solution exists only for a small subset of stochastic differential equations, stochastic numerical methods are the only way to obtain the solution. Stochastic Runge-Kutta methods are based on t he same ideas as the classic methods, but their scheme is essentially more complicated than their deterministic counterparts. This leads to difficulties when one attempts to implement them. In this paper we moti vate the approach to the implementation of these methods using source code generation as it allows achieving universality while maintaining a simplicity of code. We discuss some of the implementation details and the used programming languages and libraries. We give several examples of stochastic differential equations, used to test the program and evaluate the error of the calculations. We provide the link to the repos itory with the source code of discussed programs.},
	address = {Moscow},
	annote = {in Russian CEUR Workshop Proceedings CiteScore 2016 Q4 SNIP 2016 0.246},
	file = {work/bib/pdf/self/2017/ceur-ws-vol2064-paper04.pdf},
	issn = {16130073},
	keywords = {self, grant:rfbr:15-07-08795, grant:rfbr:16-07-00556, index:rinc, grant:rudn:5-100, index:scopus, index:rinc},
	mendeley-tags = {Scopus,РИНЦ},
	timestamp = {2020-03-30},
	type = {Conference Paper}
}

@InProceedings{kulyabov_2017_conference_ca_sympy,
	location = {Москва},
	abstract = {При моделировании таких явлений как популяционная динамика, исследование управляемых потоков и т.д. возникает проблема адаптации существующих моделей под исследуемое явление. Для этого предлагается получать новые модели из первых принципов на основе метода стохастизации одношаговых процессов. Исследование имеет вид итеративного процесса, заключающегося в получении модели и последующей её корректировке. Количество таких итераций может быть крайне большим. Целью данной работы является разработка программной реализации средствами компьютерной алгебры метода стохастизации одношаговых процессов. В работе предложено использовать систему компьютерной алгебры SymPy в качестве основы для программной реализации. На основе разработанного алгоритма показано получение стохастических дифференциальных урав- нений из вида схем взаимодействия. Результаты работы программы продемонстрированы на модели Ферхюльста.},
	address = {Москва},
	author = {Геворкян, Мигран Нельсонович and Демидова, Анастасия Вячеславовна and Велиева, Татьяна Рефатовна and Королькова, Анна Владиславовна and Кулябов, Дмитрий Сергеевич and Севастьянов, Леонид Антонович},
	booktitle = {Компьютерная алгебра},
	editor = {Абрамов, С. А. and Садыков, Т. М.},
	file = {work/bib/pdf/self/2017/ca-2017_sympy.pdf},
	keywords = {self, grant:rfbr:15-07-08795,grant:rfbr:16-07-00556,grant:rudn:5-100},
	language = {russian},
	month = {oct},
	pages = {75--81},
	publisher = {ФГБОУ ВО «РЭУ им. Г. В. Плеханова»},
	title = {Использование системы компьютерной алгебры SymPy для реализация метода стохастизации одношаговых процессов},
	year = {2017}
}

