	author = {Rosinger, Elemer Elad},
	eprint = {1006.2994},
	file = {:home/dharma/work/articles/2014/Сверхсветовое движение/cite/authors/Rosinger/Rosinger - 2010 - Two Theories of Special Relativity.pdf:pdf},
