	file = {work/bib/pdf/self/2016/convergent-2016_one-step-diagram.pdf},
	keywords = {self, index:rinc, index:vak, index:rinc},
	mendeley-tags = {ВАК,РИНЦ},
	publisher = {Фонд содействия развитию интернет-медиа, ИТ-образования, человеческого потенциала «Лига интернет-медиа»},
	timestamp = {2020-03-30},
	type = {Article}
}

